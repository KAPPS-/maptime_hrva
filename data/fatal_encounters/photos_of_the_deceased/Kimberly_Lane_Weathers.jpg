		} else if (id == "emailed") {
			document.getElementById('today-tab-emailed').src = '/images/v4.5/story/todaybox/tab-emailed-on.gif';
			document.getElementById('today-tab-discussed').src = '/images/v4.5/story/todaybox/tab-discussed-off.gif';
			document.getElementById('today-content-emailed').style.display = 'block';
			document.getElementById('today-content-discussed').style.display = 'none';
		}
	}

	function toggleEmailed (id) {
		if (id == "past7") {
			document.getElementById("past7").className="currentTab";
			document.getElementById("past24").className="";
			document.getElementById("todayTab_past7").className = "";
			document.getElementById("todayTab_past24").className = "hidden";
		} else if (id == "past24") {
			document.getElementById("past24").className="currentTab";
			document.getElementById("past7").className="";
			document.getElementById("todayTab_past24").className = "";
			document.getElementById("todayTab_past7").className = "hidden";
		}
	}
	// The Central Randomizer 1.3 (C) 1997 by Paul Houle (houle@msc.cornell.edu)
	// See:  http://www.msc.cornell.edu/~houle/javascript/randomizer.html
	cRnd.today=new Date();
	cRnd.seed=cRnd.today.getTime();
	function cRnd() {
		cRnd.seed = (cRnd.seed*9301+49297) % 233280;
		return cRnd.seed/(233280.0);
	};
	function cRand(number) {
		return Math.ceil(cRnd()*number);
	};
	// end central randomizer. -->
	function randomizeTodayTab () {
		var r = cRand(10);
		if (r <= 6){
			toggleTodayTab('discussed');
		} else {
			toggleTodayTab('emailed');
		}
	}

</script>

