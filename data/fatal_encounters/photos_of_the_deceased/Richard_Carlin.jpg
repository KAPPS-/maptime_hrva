                                        iconWidth 	= 75;
                                        iconHeight 	= 75;
	        		} else {

	        			pinIcon = highlighticons[parseInt(mtype)];

	        		}

	        	}

	  			var pin = new Microsoft.Maps.Pushpin(point, {icon: pinIcon, width: iconWidth, height: iconHeight, draggable: false, id : id,zIndex:pinIndex});
				
	            pin.id = id;
	  
	            pin.group = parseInt(mtype);
	  
	            markers.push(pin);
	  
	            map.entities.push(markers[id]);

				// ~~~~~~~~~ SET UP EVENT HANDLERS FOR THE PUSH PINS ~~~~~~~~~~~~~~~~~~~          
	            Microsoft.Maps.Events.addHandler(markers[id], 'click', function(){ var j = this.target.id;var marker_height= iconHeight; marker_clicked(j,true,this,marker_height) ;});
	            Microsoft.Maps.Events.addHandler(markers[id], 'mouseover', function(){ var j = this.target.id; var marker_height= iconHeight; marker_clicked(j,true,this,marker_height) ;});
	            id++;

	        });
	        
	        map.entities.push(infoboxLayer);
	        
	        if (times_resized==0) {

	        	num_points = $(xml).find("marker").length;
				var bump_level = 0;

				if (num_points > 100)
					bump_level = Math.floor(num_points/100) - 1;
				
				times_resized++;

	    	}

	    });


	}
    
